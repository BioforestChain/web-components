{
  "name": "@bnqkl/web-component",
  "version": "1.9.1",
  "description": "Bnqkl Web Component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/index.js",
  "es2017": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bnqkl-web-component/bnqkl-web-component.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BioforestChain/web-components.git"
  },
  "homepage": "https://github.com/BioforestChain/web-components/tree/main/core",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prepare": "patch-package && node ./scripts/fix-package.mjs && node ./scripts/mkcert.cjs -install",
    "dev": "concurrently -p \"[{name}]\" --names \"build,storybook\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn start\" \"yarn storybook\"",
    "build": "stencil build --docs & node ./scripts/fix-index.cjs",
    "start": "stencil build --docs --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "g": "node --no-warnings ./scripts/generate.mjs",
    "storybook": "start-storybook -p 6006 --ssl-cert ./localhost.pem --ssl-key ./localhost-key.pem --https",
    "storybook:limited": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@google/model-viewer": "^1.12.0",
    "@stencil/core": "^2.13.0",
    "lottie-web": "^5.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.2.0",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^18.7.14",
    "@types/three": "^0.139.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "core-js": "^3.23.5",
    "esm": "^3.2.25",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "lit-html": "^2.2.7",
    "patch-package": "^6.4.7",
    "puppeteer": "^10.0.0",
    "stencil-inline-svg": "^1.1.0",
    "sudo-prompt": "^9.2.1",
    "treeify": "^1.1.0",
    "vite": "^3.0.2"
  },
  "license": "MIT"
}
